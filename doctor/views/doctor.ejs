<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

    <title>Diabitic Clinic | Login</title>

    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="css/style.css">
    <script src="https://cdn.ckeditor.com/4.7.3/standard/ckeditor.js"></script>
  </head>

  <body>

    <nav class="navbar navbar-default">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand">Diabetic Clinic</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          
           
        </div><!--/.nav-collapse -->
      </div>
    </nav>

    <header id="header">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <h1 class="text-center">Doctor</h1>
          </div>
         
        </div>
      </div>
      
    </header>
  
   
    <section id="main">
      <div class="container">
        <div class="row">
          <h2>Patient List</h2>
          <div class="col-md-2"></div>
          <div class="col-md-8">
            
               <table class="table table-hover">
                 
                    <thead>

                    <th>Name</th> 
                    <th>Type</th>

                    </thead>

                     <tbody>
                      
                     
                     <% for(var i = 0;i < patient.length;i++) { %>
                      
                       <tr>

                           <td><%= patient[i].name %></td>
                           <td><%= patient[i].type %></td>
                         
                        
                       </tr>

                       <% } %>      

                     </tbody>

               </table>

                <div class="btn-group btn-group-justified">
                     <a href="/getpatient" class="btn btn-primary">Get</a>
                </div> 
             
          </div>
          

       </div>
      </div>
      
    </section>

  
 
    <footer id="footer">
      <p>Copyright &copy</p>
      
    </footer>
    <!--ckeditor-->
    <script>
      CKEDITOR.replace( 'pagebody' );
    </script>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="js/jquery.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    
  </body> 
</html>
